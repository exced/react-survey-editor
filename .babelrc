    "es2015",
    "react",
    "stage-0"
  ],
  "plugins": [
    [
      "import",
      {
        "libraryName": "antd",
        "libraryDirectory": "es",
        "style": true // `style: true` for less
      }
    ]
  ],
  "env": {
    "test": {
      "presets": [
        "es2015",
        "react",
        "stage-0"
